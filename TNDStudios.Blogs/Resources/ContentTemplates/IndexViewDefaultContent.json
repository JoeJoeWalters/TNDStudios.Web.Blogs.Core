{
  "Items": [
    {
      "Id": "header",
      "Content": "<div class=\"container\"><div class=\"row\"><div class=\"col-lg-12 mx-auto\">Header</div></div>"
    },
    {
      "Id": "body",
      "Content": "<div class=\"row\">{items}</div>"
    },
    {
      "Id": "indexclearfix",
      "Content": "<div class=\"clearfix {clearfix}\"></div>"
    },
    {
      "Id": "indexclearfix-medium",
      "Content": "visible-md"
    },
    {
      "Id": "indexclearfix-large",
      "Content": "visible-lg"
    },
    {
      "Id": "item",
      "Content": "<div class=\"col-lg-4 col-md-6 col-sm-12 mx-auto\"><div class=\"post-preview\"><a href=\"{controllerurl}/display/{id}\"><h2 class=\"post-title\">{name}</h2><h3 class=\"post-subtitle\">{description}</h3></a><p class=\"post-meta\">Posted by <a href=\"#\">{author}</a> on {publisheddate}</p></div><hr></div>"
    },
    {
      "Id": "footer",
      "Content": "<div class=\"row\"><div class=\"col-lg-12 mx-auto\">Footer</div></div></div>"
    }
  ]
}
